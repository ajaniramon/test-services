<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    
    <class name="es.ajanicorp.business.domain.Mensaje" table="mensaje">
        
        <id column="idMensaje" name="idMensaje">
             <generator class="identity" />
        </id>
        
        <property column="nombre" name="nombre" />
        
        <property column="email" name="email" />
        
        <property column="telefono" name="telefono" />
        
        <property column="mensaje" name="mensaje" />
        
        
    </class>
    
</hibernate-mapping>